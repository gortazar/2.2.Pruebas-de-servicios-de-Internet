node {
    stage("Build & test"){
        def mvnHome = tool 'M3'
        sh 'pwd'
        sh "cd tema1/spring-test-ejem1 ; '${mvnHome}/bin/mvn' clean package"
    }
    stage("Post") {
        archive 'tema1/spring-test-ejem1/target/*.jar'
        junit 'tema1/spring-test-ejem1/target/surefire-reports/*.xml'
    }
}